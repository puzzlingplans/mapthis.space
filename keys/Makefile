%.json:
	curl "http://taginfo.openstreetmap.org/api/4/key/values?key=$*&page=1&rp=50&sortname=count&sortorder=desc" > $@

sortedkeys.out: all
	jq -c ".data[]" *.json | sort -t ':' -k 3 -n -r > sortedkeys.out
